{
  "name": "whichcard-abroad",
  "version": "1.0.0",
  "private": false,
  "repository": "https://github.com/xingoxu/whichcard-abroad",
  "license": "MIT",
  "author": "admin@xingoxu.com",
  "main": "server.ts",
  "scripts": {
    "dev": "nodemon --exec npm run _dev",
    "_dev": "webpack && node ./.nuxt/dist/server.js",
    "build": "nuxt build && NODE_ENV=production STAGE=production webpack",
    "build:dev": "NODE_ENV=development nuxt build && NODE_ENV=production STAGE=development webpack",
    "start": "npm run build && NODE_ENV=production node ./.nuxt/dist/server.js",
    "start:dev": "npm run build:dev && node ./.nuxt/dist/server.js",
    "deploy:dev": "npm run build:dev && NODE_ENV=development sls deploy --stage dev",
    "deploy:prod": "npm run build && NODE_ENV=production sls deploy --stage prod",
    "undeploy:dev": "NODE_ENV=development sls remove --stage dev",
    "undeploy:stage": "NODE_ENV=production sls remove --stage stage",
    "undeploy:prod": "NODE_ENV=production sls remove --stage prod"
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.10.3",
    "@nuxtjs/dotenv": "^1.4.1",
    "@nuxtjs/pwa": "^3.0.0-beta.20",
    "date-fns": "^2.14.0",
    "nuxt": "^2.12.2",
    "nuxt-buefy": "^0.3.30"
  },
  "devDependencies": {
    "@nuxt/types": "^0.7.9",
    "@nuxt/typescript-build": "^1.0.3",
    "@nuxtjs/google-analytics": "^2.3.0",
    "@types/aws-lambda": "^8.10.51",
    "@types/aws-serverless-express": "^3.3.3",
    "@types/cheerio": "^0.22.18",
    "@types/cookie-parser": "^1.4.1",
    "@types/express": "^4.17.6",
    "@types/node": "^12.12.42",
    "aws-lambda": "^1.0.6",
    "aws-sdk": "^2.679.0",
    "aws-serverless-express": "^3.3.6",
    "axios": "^0.19.2",
    "body-parser": "^1.19.0",
    "cheerio": "^1.0.0-rc.3",
    "cookie-parser": "^1.4.4",
    "dotenv": "^8.0.0",
    "express": "^4.17.3",
    "express-asyncify": "^1.0.0",
    "fibers": "^5.0.0",
    "lodash": "^4.17.13",
    "node-sass": "^4.12.0",
    "nodemon": "^2.0.4",
    "npm-run-all": "^4.1.5",
    "nuxt-property-decorator": "^2.3.0",
    "qs": "^6.9.4",
    "sass": "^1.26.5",
    "sass-loader": "^8.0.2",
    "serverless": "^1.71.1",
    "serverless-apigw-binary": "^0.4.4",
    "serverless-domain-manager": "^4.1.0",
    "serverless-dotenv-plugin": "^2.1.1",
    "serverless-offline": "^6.3.0",
    "serverless-s3-sync": "^1.8.0",
    "ts-loader": "^7.0.4",
    "ts-node": "^8.2.0",
    "vue": "^2.6.10",
    "vue-class-component": "^7.1.0",
    "vuex": "^3.1.1",
    "vuex-class": "^0.3.2",
    "webpack": "^4.33.0",
    "webpack-cli": "^3.3.4",
    "webpack-node-externals": "^1.7.2",
    "webpackbar": "^3.2.0"
  },
  "nodemonConfig": {
    "env": {
      "NODE_ENV": "development",
      "HOT_RELOAD": "true"
    },
    "ignore": [
      ".git",
      "node_modules/**/node_modules"
    ],
    "watch": [
      "src/api/**/*.*",
      "app.ts",
      "server.ts",
      "serverless.ts"
    ]
  }
}
